{
  "name": "2digits-blog-case",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:web": "next dev --turbo",
    "dev": "turbo dev:web",
    "build:web": "next build",
    "build": "turbo build:web",
    "start": "next start",
    "types": "tsc --noEmit",
    "fix": "pnpm '/(lint|format):fix$/'",
    "format": "prettier \"**/*\" --ignore-unknown --check --cache",
    "format:fix": "prettier \"**/*\" --ignore-unknown --write --cache",
    "lint": "eslint . --cache --max-warnings 0",
    "lint:fix": "eslint . --fix --cache",
    "check": "turbo --continue format lint types",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
    "graphql": "^16.9.0",
    "graphql-request": "^7.1.0",
    "html-react-parser": "^5.1.12",
    "next": "^14.2.5",
    "ofetch": "^1.3.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@2digits/eslint-config": "^2.4.6",
    "@2digits/prettier-config": "^2.2.6",
    "@2digits/tsconfig": "^0.5.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/import-types-preset": "^3.0.0",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.2.3",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.9",
    "turbo": "^2.0.12",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.7.0",
  "prettier": "@2digits/prettier-config"
}
